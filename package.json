{
  "name": "celarium",
  "version": "0.0.2",
  "description": "celarium is a tool to create CRUD APIs with a tree-structure in mere minutes",
  "main": "src/index.js",
  "template": {
    "extends": [
      "node"
    ]
  },
  "files": [
    "example.js",
    "src/**/*.js"
  ],
  "bin": {
    "celarium": "./src/bin.js"
  },
  "scripts": {
    "test": "mocha -A",
    "example": "celarium node:$PWD/example.js",
    "cov": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkg20001/celarium.git"
  },
  "keywords": [
    "rest",
    "crud",
    "auth",
    "generator"
  ],
