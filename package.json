  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mkg20001/celarium/issues"
  },
  "homepage": "https://github.com/mkg20001/celarium#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-aegir-standalone": "^21.3.0",
    "eslint-config-oclif": "^3.1.0",
    "mkgs-tool": "^0.1.8",
    "mocha": "*",
    "nyc": "*"
  }
